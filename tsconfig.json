{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5"],
    "module": "commonjs",
    "declaration": false,
    "sourceMap": false,
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true
  },
  "exclude": ["tests"]
}
